<div class="debug-info-window">
  <fieldset>
        <legend>debug control</legend>
        <span title="test video" class="fa button button-vidtest">vid tst</span>
        <span title="force video draw" class="fa button button-forcedraw">frc drw</span>
    </fieldset>
</div>
<div class="debug-info-window">
<fieldset>
    <legend>[info]</legend>
    <%
        var kls = (data.error) ? "error" : "info";
    %>
    <span class="<%= kls %>"><%= data.message %></span>
</fieldset>
</div>